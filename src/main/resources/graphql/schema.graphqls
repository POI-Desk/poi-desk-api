type Query{
    getAllUsers: [User]
    getUserById(id: ID): User
    getRolesOfUser(id: ID): [Role]
    allBookings: [Booking]
    getBookingsByDate(date: String): [Booking]
}

type Mutation{
    changeUsername(id: String, name:String): User
}

type User{
    pk_userid: ID
    username: String
    roles: [Role]
    bookings: [Booking]
}

type Role{
    pk_roleid: ID
    description: String
}

type Booking{
    pk_bookingid: ID
    bookingnumber: Int
    date: String
    user: User
}